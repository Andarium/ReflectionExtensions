    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="4.10.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ReflectionExtensions\ReflectionExtensions.csproj" />
  </ItemGroup>

</Project>
